<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.navi.mobile">
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".components.login.Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".components.comprador.MainComprador"/>
        <activity android:name=".components.SelectUserType_SignUp" />
        <activity android:name=".components.sign_up.comprador.CompradorInfos_SignUp" />
        <activity android:name=".components.sign_up.comprador.CompradorOtherInfos_SignUp" />
        <activity android:name=".components.sign_up.comprador.CompradorEnderecoInfos_SignUp" />
        <activity android:name="br.com.navi.mobile.components.sign_up.vendedor.VendedorInfos_SignUp" />
        <activity android:name="br.com.navi.mobile.components.sign_up.vendedor.VendedorOtherInfos_SignUp" />
        <activity android:name="br.com.navi.mobile.components.sign_up.vendedor.VendedorLojaInfos_SignUp" />
        <activity android:name="br.com.navi.mobile.components.sign_up.vendedor.VendedorEnderecoInfos_SignUp" />
    </application>

</manifest>